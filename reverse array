#include<stdio.h>
#include<math.h>
int main()
{
    int i,n,p,q;
    printf("Enter leghth of array\n");
    scanf("%d",&n);
    printf("Enter values of array\n");
    int a[n];
    for(i=0;i<n;i++)
     scanf("%d",&a[i]);
    p=0;
    q=n-1;
    if(n%2==0)
     for(i=0;i<(n+1)/2;i++)
     {
          a[p]=a[p]+a[q];
          a[q]=a[p]-a[q];
          a[p]=a[p]-a[q];
          p+=1;
          q=q-1;
     }
     else
     for(i=0;i<(n-1)/2;i++)
     {
         a[p]=a[p]+a[q];
          a[q]=a[p]-a[q];
          a[p]=a[p]-a[q];
          p+=1;
          q=q-1;
     }
    printf("The new array order is\n");
    for(i=0;i<n;i++)
     printf("%d ",a[i]);
    return 0;
}
