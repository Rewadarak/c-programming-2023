#include<stdio.h>
#include<math.h>
int main()
{
    int i,j,n,k,l;
    printf("Enter leghth of array\n");
    scanf("%d",&n);
    printf("Enter values of array\n");
    int a[n];
    for(i=0;i<n;i++)
     scanf("%d",&a[i]);
    printf("Enter a number ");
    scanf("%d",&k);
    l=0;
    for(i=0;i<n;i++)
      if(a[i]==k)
       l+=1;
    printf("The number of times %d has been repeated is %d\n",k,l);
    return 0;
}
