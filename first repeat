#include<stdio.h>
#include<math.h>
int main()
{
    int i,j,n;
    printf("Enter leghth of array\n");
    scanf("%d",&n);
    printf("Enter values of array\n");
    int a[n];
    for(i=0;i<n;i++)
     scanf("%d",&a[i]);
    for(i=0;i<n;i++)
    {
     for(j=n;j>i;j--)
      if(a[i]==a[j])
       printf("The number that repeats first is %d",a[i]);
     break;
    }
    return 0;
}
