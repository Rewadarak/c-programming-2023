#include<stdio.h>
#include<math.h>
int main()
{
    int i,j,n;
    printf("Enter leghth of array\n");
    scanf("%d",&n);
    printf("Enter values of array\n");
    int a[n];
    for(i=0;i<n;i++)
     scanf("%d",&a[i]);
    for(i=0;i<n-1;i++)
     for(j=0;j<n-i-1;j++)
      if(a[j]>a[j+1])
      {
          a[j]=a[j+1]+a[j];
          a[j+1]=a[j]-a[j+1];
          a[j]=a[j]-a[j+1];
      }
    printf("The sorted array in ascending order is\n");
    for(i=0;i<n;i++)
     printf("%d ",a[i]);
    return 0;
}
